 204  * Training constraints
 205  training_constraint_unskilled_limit(t, s, s).. Train(t, 'Unskilled', 'Semi-skilled')=l= training_limit('fixed');
 206  training_constraint_unskilled_workforce(t, s, s).. Train(t, 'Unskilled', 'Semi-skilled')=l= W(t, 'Unskilled');
 207   
 208  training_constraint_semiskilled_limit(t, s, s).. Train(t, 'Semi-skilled', 'Skilled')=l= training_theshold('fixed')* W(t, 'Skilled');
 209  training_constraint_semiskilled_workforce(t, s, s).. Train(t, 'Semi-skilled', 'Skilled')=l= W(t, 'Semi-skilled')+ Train(t, 'Unskilled', 'Semi-skilled');
 210   
 211  * demotion constraints
 212  demote_constraint_semiskilled(t, s, s).. Demote(t, 'Semi-skilled', 'Unskilled')=l= W(t, 'Semi-skilled');
 213  demote_constraint_skilled(t, s, s).. Demote(t, 'Skilled', 'Semi-skilled')+ Demote(t, 'Skilled', 'Unskilled')=l= W(t, 'Skilled');
 214   
 215  * defining costs
 216  * training
 217  training_costs.. trainincost=e= sum(t, training_cost('Unskilled', 'Semi-skilled')* Train(t, 'Unskilled', 'Semi-skilled')+ training_cost('Semi-skilled', 'Skilled')* Train(t, 'Semi-skilled', 'Skilled'));
 218   
 219  *parttime
 220  parttime_costs.. parttimecost=e= sum((t, s), parttime_cost(s)* P(t, s));
 221   
 222  * layoff
 223  layoff_costs.. layoffcost=e= sum((t, s), layoff_cost(s)* L(t, s));
 224   
 225  * overhire
 226  overhire_costs.. overhiringcost=e= sum((t, s), over_hiring_cost(s)* O(t, s));
 227   
 228  * lay off objective for the first part
 229  layoff.. Z_first=e= sum((t, s), L(t, s));
 230   
 231  * total cost
 232  cost.. z_second=e= trainincost+ parttimecost+ layoffcost+ overhiringcost;

 191  Hiring_constraint_normal(t, s).. H(t, s)=l= hiring_limit(s);
 192   
 193  * over
 194  Hiring_constraint_over(t).. sum(s, O(t, s))=l= overhiring_limit('fixed');
 195   
 196  * part
 197  Hiring_constraint_part(t).. sum(s, P(t, s))=l= parttime_limit('fixed');
 198   
 199  * lay off constraint
 200  layoff_constraint(t, s).. L(t, s)=l= W(t, s);
 201   

 184  * normal
 185  Hiring_constraint_normal(t, s).. H(t, s)=l= hiring_limit(s);
 186   
 187  * over
 188  Hiring_constraint_over(t).. sum(s, O(t, s))=l= overhiring_limit('fixed');
 189   
 190  * part
 191  Hiring_constraint_part(t).. sum(s, P(t, s))=l= parttime_limit('fixed');
 192   
 193  * lay off constraint
 194  layoff_constraint(t, s).. L(t, s)=l= W(t, s);

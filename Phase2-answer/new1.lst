 198  * Training constraints
 199  training_constraint_unskilled_limit(t, s, s).. Train(t, 'Unskilled', 'Semi-skilled')=l= training_limit('fixed');
 200  training_constraint_unskilled_workforce(t, s, s).. Train(t, 'Unskilled', 'Semi-skilled')=l= W(t, 'Unskilled');
 201   
 202  training_constraint_semiskilled_limit(t, s, s).. Train(t, 'Semi-skilled', 'Skilled')=l= training_theshold('fixed')* W(t, 'Skilled');
 203  training_constraint_semiskilled_workforce(t, s, s).. Train(t, 'Semi-skilled', 'Skilled')=l= W(t, 'Semi-skilled')+ Train(t, 'Unskilled', 'Semi-skilled');
 204   
 205  * demotion constraints
 206  demote_constraint_semiskilled(t, s, s).. Demote(t, 'Semi-skilled', 'Unskilled')=l= W(t, 'Semi-skilled');
 207  demote_constraint_skilled(t, s, s).. Demote(t, 'Skilled', 'Semi-skilled')+ Demote(t, 'Skilled', 'Unskilled')=l= W(t, 'Skilled');
 208   
 209  * defining costs
 210  * training
 211  training_costs.. trainincost=e= sum(t, Train(t, 'Unskilled', 'Semi-skilled'));
 212   
 213  *parttime
 214  parttime_costs.. parttimecost=e= sum((t, s), parttime_cost(s)* P(t, s));
 215   
 216  * layoff
 217  layoff_costs.. layoffcost=e= sum((t, s), layoff_cost(s)* L(t, s));
 218   
 219  * overhire
